#!/bin/bash

#pins layout
export RELAY_PIN1=22
export RELAY_PIN2=23
export RELAY_PIN3=24
export RELAY_PIN4=25
export RELAY_PIN5=21

#devices_layout
export RELAY_DOUBLE_SOCKET=$RELAY_PIN1
export RELAY_HPS=$RELAY_PIN2
export RELAY_TSENSOR=$RELAY_PIN3
export RELAY_SINGLE_SOCKET=$RELAY_PIN4
export RELAY_LED=$RELAY_PIN5


gpio mode $1 out
gpio write $1 $2


